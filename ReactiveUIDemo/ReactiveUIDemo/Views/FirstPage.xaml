<?xml version="1.0" encoding="utf-8" ?>
<reactiveui:ReactiveContentPage
             x:TypeArguments="vm:FirstViewModel"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:reactiveui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
             xmlns:vm="clr-namespace:ReactiveUIDemo.ViewModels"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="ReactiveUIDemo.Views.FirstPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry Text="{Binding Message}" Margin="2" Placeholder="Enter message to second page"/>
            <Button Text="Go to second page" Command="{Binding NavigateCommand}" Margin="2"/>
        </StackLayout>
    </ContentPage.Content>
</reactiveui:ReactiveContentPage>